  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>

	<PropertyGroup>
		<PackageId>clojure.core.async</PackageId>
		<RootNamespace>clojure.core</RootNamespace>
		<Title>clojure.core.async</Title>
		<Product>clojure.core.async</Product>
		<AssemblyTitle>clojure.core.async</AssemblyTitle>
		<Authors>David Miller and ClojureCLR contributors</Authors>
		<Description>A port of core.async to ClojureCLR</Description>
		<Copyright>Copyright Â© Rich Hickey, David Miller and ClojureCLR contributors</Copyright>
		<PackageLicenseExpression>EPL-1.0</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/clojure/clojure.tools.namesapce</RepositoryUrl>
		<Company>ClojureCLR contributors</Company>
		<PackageTags>Clojure;ClojureCLR</PackageTags>
		<Version>1.6.673.2</Version>
